class Solution {
public:
    bool threeConsecutiveOdds(vector<int>& arr) {
        for(int i = 0;; i++){
            if(i + 2 >= arr.size()) break;
            int c = arr[i], d = arr[i + 1], e = arr[i + 2];
            if((c % 2) && (d % 2) && (e % 2)) return true;
        }
        return false;
    }
};
